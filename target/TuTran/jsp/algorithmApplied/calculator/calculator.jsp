<div class="container-fuild">
	<div class="col-xs-4"></div>
	<div class="calculator col-xs-8">
		<div class="calScreen">
			<form class="form-horizontal">
			  <div class="form-group">
			    <div class="col-xs-6">
			      <p id="txtCal" class="form-control-static">20+15</p>
			    </div>
			  </div>
			</form>
		</div>
		<div class="groupOfButton">
			<div class="row">
			<button id="7" class="btn btn-primary col-xs-1">7</button>
			<button id="8" class="btn btn-primary col-xs-1">8</button>
			<button id="9" class="btn btn-primary col-xs-1">9</button>
			<button id="/" class="btn btn-primary col-xs-1">/</button>
			</div>
			<div class="row">
				<button id="4" class="btn btn-primary col-xs-1">4</button>
				<button id="5" class="btn btn-primary col-xs-1">5</button>
				<button id="6" class="btn btn-primary col-xs-1">6</button>
				<button id="*" class="btn btn-primary col-xs-1">*</button>
				<button id="deleteSingleChar" class="btn btn-danger col-xs-1"><i class="fa fa-remove"></i></button>
			</div>
			<div class="row">
				<button id="1" class="btn btn-primary col-xs-1">1</button>
				<button id="2" class="btn btn-primary col-xs-1">2</button>
				<button id="3" class="btn btn-primary col-xs-1">3</button>
				<button id="-" class="btn btn-primary col-xs-1">-</button>
				<button id="deleteAll" class="btn btn-danger col-xs-1">C</button>
			</div>
			<div class="row">
				<button id="0" class="btn btn-primary col-xs-1">0</button>
				<button id="(" class="btn btn-primary col-xs-1">(</button>
				<button id=")" class="btn btn-primary col-xs-1">)</button>
				<button id="+" class="btn btn-primary col-xs-1">+</button>
				<button id="=" class="btn btn-default col-xs-1">=</button>
			</div>
		</div>
	</div>
</div>